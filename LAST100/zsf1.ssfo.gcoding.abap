SELECT LIFNR NAME1 LAND1 ORT01 PSTLZ
       INTO ITAB FROM LFA1
       WHERE LIFNR EQ SF_VENDOR.
ENDSELECT.
IF SY-SUBRC EQ 0.
  SELECT ADRNR INTO VADRNR FROM LFA1
       WHERE LIFNR EQ SF_VENDOR.
  ENDSELECT.
  ELSE.
    RAISE NOT_FOUND.
  ENDIF.
  SFSYDATE = SY-DATUM.
  SFSYTIME = SY-TIMLO.





















